        <script><![CDATA[var getDepartment = Class.create();
getDepartment.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
getdepartment: function() {
		var siteName;
		var department = this.getParameter('sysparm_vendor');
	var gr1 = new GlideRecord('sys_user');
	gr1.addQuery('sys_id', department);
		gr1.query();
		if (gr1.next())
			//query vendor name for sysid passed from client script
			siteName = gr1.name;
	
		        return siteName;
 	},
	
    type: 'getDepartment'
});]]></script>
