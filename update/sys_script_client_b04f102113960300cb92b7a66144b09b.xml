        <name>Sitename, city and manager auto populate</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {

	if(oldValue!=newValue)
		{
			var caller = g_form.getReference('tech_name',doDetails);
		}
	function doDetails(caller)
	{
		var test = caller.department.name;
		var test1 = caller.city;
		g_form.setValue('site_name',test);
		g_form.setValue('site_state',test1);
	}
	if(newValue=='')
		{
			g_form.setValue('site_name','');
			g_form.setValue('site_state','');
		}
	
}]]></script>
